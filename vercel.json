{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/upImage",
      "dest": "/api/image.js"
    },
    {
      "src": "/api/sendMail",
      "dest": "/api/mail.js"
    },
    {
      "src": "/api/stripeIntent",
      "dest": "/api/stripe.js"
    }
  ]
}
