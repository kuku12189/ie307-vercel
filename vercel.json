{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/upImage",
      "dest": "/api/image.js"
    },
    {
      "src": "/api/sendMail",
      "dest": "/api/mail.js"
    },
    {
      "src": "/api/stripeIntent",
      "dest": "/api/stripe.js"
    },
    {
      "src": "/api/register",
      "dest": "/api/register.js"
    },
    {
      "src": "/api/login",
      "dest": "/api/login.js"
    },
    {
      "src": "/api/updateUserInfo",
      "dest": "/api/updateUserInfo.js"
    },
    {
      "src": "/api/updatePic",
      "dest": "/api/updatePic.js"
    },
    {
      "src": "/api/addSale",
      "dest": "/api/addSale.js"
    },
    {
      "src": "/api/subSale",
      "dest": "/api/subSale.js"
    },
    {
      "src": "/api/forgotPw",
      "dest": "/api/forgotPw.js"
    },
    {
      "src": "/api/getUserWithRole",
      "dest": "/api/getUserWithRole.js"
    },
    {
      "src": "/api/updateRole",
      "dest": "/api/updateRole.js"
    },
    {
      "src": "/api/getProducts",
      "dest": "/api/getProducts.js"
    }
  ]
}
